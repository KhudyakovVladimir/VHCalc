<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".About" android:background="#73B8E6">
    <ImageButton
            android:layout_width="327dp"
            android:layout_height="348dp" app:srcCompat="@drawable/icon_calc" android:id="@+id/imageButton"
            android:background="#FF73B8E6"
            app:layout_constraintTop_toBottomOf="@+id/textView3" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="  Copyright by Hudyakov Vladimir "
            android:layout_width="342dp"
            android:layout_height="48dp" android:id="@+id/textView2"
            android:textSize="18sp" android:textColor="#183D57" android:fontFamily="@font/audiowide"
            app:layout_constraintTop_toBottomOf="@+id/textView4" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="2020"
            android:layout_width="74dp"
            android:layout_height="46dp" android:id="@+id/textView3"
            android:textSize="18sp" android:fontFamily="@font/audiowide"
            android:textColor="#183D57"
            app:layout_constraintTop_toBottomOf="@+id/textView2" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.471"/>
    <TextView
            android:text="VH Calc"
            android:layout_width="121dp"
            android:layout_height="51dp" android:id="@+id/textView4"
            android:textSize="24sp" android:textColor="#183D57"
            android:fontFamily="@font/audiowide"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="24dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>
